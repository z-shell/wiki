{
  "name": "wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "clear": "docusaurus clear; command rm -rf node_modules build pnpm-lock.yaml",
    "crowdin:check": "crowdin lint && crowdin status",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "crowdin:upload": "pnpm crowdin upload sources --delete-obsolete",
    "deploy": "docusaurus deploy",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "write-community-version": "docusaurus docs:version:community",
    "write-docs-version": "docusaurus docs:version",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-translations:force": "docusaurus write-translations --override"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/faster": "3.8.1",
    "@docusaurus/plugin-client-redirects": "3.8.1",
    "@docusaurus/plugin-ideal-image": "3.8.1",
    "@docusaurus/plugin-pwa": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@mdx-js/react": "3.1.0",
    "asciinema-player": "3.10.0",
    "clsx": "2.1.1",
    "prism-react-renderer": "2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-spinners": "0.17.0"
  },
  "devDependencies": {
    "@crowdin/cli": "4.7.1",
    "@docusaurus/eslint-plugin": "3.8.1",
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@eslint/js": "9.29.0",
    "eslint": "9.29.0",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.2.0",
    "gray-matter": "4.0.3",
    "stylelint": "16.21.0",
    "stylelint-color-format": "1.1.0",
    "stylelint-config-css-modules": "4.4.1",
    "stylelint-config-standard": "38.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "core-js",
      "core-js-pure",
      "sharp"
    ]
  }
}
