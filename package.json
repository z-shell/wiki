{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "crowdin": "crowdin",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload sources && crowdin download",
    "prebuild": "test \"$CF_PAGES\" = 1 && npx pnpm install --prod || echo Skipping pnpm install",
    "clean": "pnpm exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "check": "pnpm exec ./.trunk/bin/trunk check --all",
    "test": "pnpm run check && pnpm run build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-5214",
    "@docusaurus/plugin-ideal-image": "0.0.0-5214",
    "@docusaurus/plugin-pwa": "0.0.0-5214",
    "@docusaurus/preset-classic": "0.0.0-5214",
    "@loadable/component": "5.15.2",
    "@mdx-js/react": "1.6.22",
    "asciinema-player": "3.0.1",
    "clsx": "1.2.1",
    "husky": "8.0.1",
    "prism-react-renderer": "1.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spinners": "0.13.3"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@docusaurus/eslint-plugin": "0.0.0-5214",
    "@docusaurus/module-type-aliases": "0.0.0-5214",
    "@tsconfig/docusaurus": "1.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "prettier-plugin-sh": "0.12.6",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-csstree-validator": "2.0.0",
    "typescript": "4.7.4"
  },
  "packageManager": "pnpm@7.5.2",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@mdx-js/react": "1.6",
        "rehype-katex": "6",
        "remark-math": "3",
        "react": "17"
      }
    },
    "overrides": {
      "trim@<0.0.3": ">=0.0.3",
      "got@<11.8.5": ">=11.8.5"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
