{
  "name": "wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "trunk": "trunk",
    "crowdin": "crowdin",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "crowdin:check": "crowdin lint && crowdin status",
    "crowdin:upload": "pnpm crowdin upload sources --delete-obsolete",
    "clean": "pnpm exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "upgrade": "pnpm upgrade -i -L",
    "check": "trunk check"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.1",
    "@docusaurus/plugin-ideal-image": "2.4.1",
    "@docusaurus/plugin-pwa": "2.4.1",
    "@docusaurus/preset-classic": "2.4.1",
    "@docusaurus/theme-mermaid": "2.4.1",
    "@loadable/component": "5.15.3",
    "@mdx-js/react": "1.6.22",
    "asciinema-player": "3.5.0",
    "clsx": "2.0.0",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "0.13.8"
  },
  "devDependencies": {
    "@crowdin/cli": "^3.13.0",
    "@docusaurus/eslint-plugin": "^2.4.1",
    "@docusaurus/module-type-aliases": "2.4.3",
    "@docusaurus/types": "2.4.3",
    "@trunkio/launcher": "^1.2.7",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "stylelint": "^15.10.2",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "typescript": "5.1.6"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@8.6.12",
  "peerDependencyRules": {
    "allowedVersions": {
      "react": ">=17",
      "react-dom": ">=17",
      "stylelint": ">=14"
    }
  },
  "pnpm": {
    "overrides": {
      "got@<11.8.5": ">=11.8.5",
      "trim@<0.0.3": ">=0.0.3",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1"
    }
  }
}
