{
  "name": "z-shell-wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "crowdin": "crowdin",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-docs-version": "docusaurus docs:version",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations",
    "write-community-version": "docusaurus docs:version:community",
    "write-ecosystem-version": "docusaurus docs:version:ecosystem",
    "crowdin:sync": "docusaurus write-translations && crowdin upload sources && crowdin download",
    "crowdin:check": "crowdin lint && crowdin status",
    "prebuild": "test \"$CF_PAGES\" = 1 && npx pnpm install --prod --store=node_modules/.pnpm-store || echo Install not required...",
    "clean": "pnpm exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm audit",
    "prepare": "husky install",
    "check": "pnpm trunk check",
    "test": "pnpm run check --all",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-5272",
    "@docusaurus/plugin-ideal-image": "0.0.0-5272",
    "@docusaurus/plugin-pwa": "0.0.0-5272",
    "@docusaurus/preset-classic": "0.0.0-5272",
    "@docusaurus/types": "0.0.0-5272",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/component": "5.15.2",
    "@mdx-js/react": "1.6.22",
    "@primer/react": "35.8.0",
    "asciinema-player": "3.0.1",
    "clsx": "1.2.1",
    "husky": "8.0.1",
    "prism-react-renderer": "1.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spinners": "0.13.4",
    "remark-math": "3.0.1"
  },
  "devDependencies": {
    "@crowdin/cli": "3.7.10",
    "@docusaurus/eslint-plugin": "0.0.0-5272",
    "@docusaurus/module-type-aliases": "0.0.0-5272",
    "@primer/stylelint-config": "^12.7.0",
    "@types/jest": "^28.1.8",
    "@types/node": "18.0.6",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-primer-react": "^1.0.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "styled-components": "5.3.5",
    "stylelint": "14.11.0",
    "stylelint-color-format": "1.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-standard": "28.0.0",
    "typescript": "4.8.2"
  },
  "packageManager": "pnpm@7.9.5",
  "pnpm": {
    "overrides": {
      "trim@<0.0.3": ">=0.0.3",
      "got@<11.8.5": ">=11.8.5",
      "glob-parent@<6.0.1": ">=6.0.1"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
